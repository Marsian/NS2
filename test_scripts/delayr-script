#sh delayr-script fileheader range source1 dest1 source2 dest2 beforehandovertime afterhandovertime
#sh delayr-script random 10 68 70 69 71 29.5 96
file=$1
for n in $(seq 1 $2)
do 
	file=$file$n.tr
	perl delay $file $3 $4 $7 $8>> random-delay
	perl delay $file $5 $6 $7 $8>> random-delay
	file=$1
done
perl avg random-delay $3 $5 > avg-delay	
